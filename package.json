{
  "name": "financial-analytics",
  "version": "0.3.3",
  "author": {
    "name": "Tim Bilalov",
    "email": "t-moore@mail.ru",
    "url": "http://unfriend.lh1.in/"
  },
  "scripts": {
    "api:build:dev": "SERVICE=api rollup -c -w",
    "api:server": "./bin/check-postgres.sh && MODE=development nodemon --watch public/api/app.js public/api/app.js",
    "api:safe-start": "mkdir -p public/api && touch public/api/app.js",
    "api:dev": "npm run api:safe-start && concurrently \"npm run api:build:dev\" \"npm run api:server\"",
    "frontend:lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "frontend:test": "jest --silent --coverage true",
    "frontend:build": "SERVICE=frontend rollup -c",
    "frontend:dev": "SERVICE=frontend rollup -c -w",
    "frontend:assets": "sirv public",
    "dev": "npm run api:safe-start && concurrently \"npm run api:build:dev\" \"npm run api:server\" \"npm run frontend:dev\"",
    "start": "npm run dev"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@tsconfig/svelte": "^1.0.10",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "babel-jest": "^27.0.2",
    "concurrently": "^6.5.1",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-typescript-enum": "^2.0.11",
    "jest": "^27.0.4",
    "nodemon": "^2.0.15",
    "nollup": "^0.19.0",
    "rollup": "^2.75.5",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-hot": "^0.1.1",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-svelte-hmr": "0.0.15",
    "rollup-plugin-svelte-hot": "^1.0.0-8",
    "rollup-plugin-terser": "^7.0.0",
    "sirv-cli": "^1.0.0",
    "surge": "^0.21.7",
    "svelte-check": "^1.3.0",
    "svelte-preprocess": "^4.7.0",
    "ts-jest": "^27.0.3",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "chart.js": "^2.9.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "effector": "^21.7.5",
    "ejs": "^3.1.6",
    "express": "^4.17.2",
    "express-fingerprint": "^1.2.2",
    "express-graphql": "^0.12.0",
    "graphql": "^16.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "lz-string": "^1.4.4",
    "moment": "^2.28.0",
    "pg": "^8.7.1",
    "svelte": "^3.48.0",
    "svelte-routing": "^1.6.0"
  }
}
